<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/Carview2"
        android:layout_width="350dp"
        android:layout_height="167dp"
        android:layout_marginTop="16dp"
        app:cardBackgroundColor="#FF8A85"
        app:cardCornerRadius="13dp"
        app:cardElevation="10dp"
        app:contentPadding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="332dp"
            android:layout_height="151dp"
            android:background="#fff">


            <TextView
                android:id="@+id/fechaitemTransacciones"
                android:layout_width="192dp"
                android:layout_height="28dp"
                android:padding="3dp"
                android:text="Fecha"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.085"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="8dp" />

            <TextView
                android:id="@+id/cuentaclienrecycler"
                android:layout_width="192dp"
                android:layout_height="28dp"
                android:padding="3dp"
                android:text="Tipo transaccion"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.085"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="44dp" />

            <TextView
                android:id="@+id/cuentacliente"
                android:layout_width="194dp"
                android:layout_height="30dp"
                android:padding="3dp"
                android:text="Monto transaccio"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.086"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="80dp" />

            <TextView
                android:id="@+id/cedulaclienteefwef"
                android:layout_width="196dp"
                android:layout_height="24dp"
                android:padding="3dp"
                android:text="Cedula cliente"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.088"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="120dp" />

            <TextView
                android:id="@+id/cedulacliente"
                android:layout_width="122dp"
                android:layout_height="23dp"
                android:background="#FF8A85"
                android:padding="3dp"
                android:text="id transaferencia"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.99"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="8dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>